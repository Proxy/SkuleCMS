%div#skulebar
	= link_to "Skule.ca", "/", :id => "skule"
	- form_for :club, @search, :url => {:controller => 'search', :action => 'search', :club_id => (club_id||nil)}  do |f|
		= text_field_with_auto_complete :search, :search, {}, { :url => formatted_search_path( :js), :select => :entry_name, :after_update_element => "submit", :frequency => 0.4, :method => :get, :param_name => 'search' }
	-#
		%ul#bookmarks
			%li= link_to "Google", "http://www.google.com", :class => "google"
			%li= link_to "Facebook", "http://www.facebook.com", :class => "facebook"
			%li= link_to "More", "#", :class => "more"
	
	%ul#message= flash[:notice]																																																																					

	%div#user	
		-if current_admin
			= link_to current_admin.login, edit_admin_path(:current), :id=> 'user'
			= link_to "Logout", logout_path
		-else
			=link_to "Register", new_admin_path
			| 
			=link_to "Login", login_path

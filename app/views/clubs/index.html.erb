<h2>Clubs</h2>



<% @clubs.each do |club| %>

<div class="club">
	<img src="logo" />
	<h3><%= link_to club.name, :action => 'show', :id => club  %></h3>
	<span><%=h club.tagline || "Tagline" %></span>
	<ul>
	<% club.tags.each do |tag| %>
		<li><a href="#"><%= tag %></a></li>
	<% end if club.tags %>
		<li><a href="#">Tag</a></li>
	</ul>
</div>

<% end %>



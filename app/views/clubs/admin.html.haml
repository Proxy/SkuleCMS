%h2= @page_title

%table
	%tr
		%th Name
		%th Actions

	- @clubs.each do |club|

		%tr
			%td=h club.name 
			%td
				= link_to 'Events', club_events_path(club)
				= link_to 'Files', club_files_path(club) 
				= link_to 'Galleries', club_gallery_path(club)
				= link_to 'Groups', club_groups_path(club)
				= link_to 'Show', club 
				= link_to 'Edit', edit_club_path(club)
				= link_to 'Destroy', club, :confirm => 'Are you sure?', :method => :delete

= link_to 'New club', new_club_path

<%= include_tiny_mce_if_needed %>

<h1>New large_post</h1>


<% form_for([:club, @large_post], :url => {:action=> 'create'}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content, :class => "mceEditor" %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>


<%= link_to 'Back', :action => 'admin' %>
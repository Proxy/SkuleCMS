<h2>Gallery</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Locked</th>
    <th>User</th>
  </tr>

<% @albums.each do |album| %>
  <tr>
    <td><%=h album.name %></td>
    <td><%=h album.desc %></td>
    <td><%=h album.locked %></td>
    <td><%=h album.user.name %></td>
    <td><%= link_to 'Show', club_album_path(album.club, album) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Back', club_path(@club) %>
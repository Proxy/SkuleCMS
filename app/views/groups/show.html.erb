<p>
  <b>Name:</b>
  <%=h @group.name %>
</p>
<p>
  <b>Parent:</b>
  <%=h (@group.parent.blank?) ? "" : @group.parent.name %></td>
</p>
<p>
  <b>Description:</b>
  <%=h @group.misc %>
</p>

<p>
  <b>Members:</b>
  <ul>
    <% @group.memberships.each do |member| %>
         <li><%=h member.user.name %>  
    <% end %>
  </ul>
</p>

<% if !@page.nil? %>
	<%= link_to 'Page', club_page_path(@club, @page) %> |
<% end %>
<%= link_to 'Back', :action => 'index' %>
<p>
  <b>Name:</b>
  <%=h @group.name %>
</p>

<p>
  <b>Parent:</b>
  <%=h (@group.parent.blank?) ? "" : @group.parent.name %></td>
</p>
<p>
  <b>Misc:</b>
  <%=h @group.misc %>
</p>

<p>
  <b>Members:</b>
  <ul>
    <% @group.memberships.each do |member| %>
         <li><%=h member.user.name %>  
         <%= link_to 'Kick', kick_club_group_path(@club, @group, :member => member.id), :confirm => 'Are you sure?', :method => :delete %></li>
    <% end %>
  </ul>
</p>

<% unless @group.is_member_list? %>
<%= link_to 'Edit', :action => 'edit', :id => @group %> |
<% end %>
<% if !@page.nil? %>
	<%= link_to 'Page', club_page_path(@club, @page) %> |
<% end %>
<%= link_to 'Back', :action => 'admin' %>
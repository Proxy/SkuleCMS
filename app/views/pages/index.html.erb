<h2>Listing pages</h2>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>

<% @pages.each do |page| %>
  <tr>
    <td><%=h page.title %></td>
    <td><%= page.content %></td>
    <td><%= link_to 'Show', club_page_path(@club, page) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Back', club_path(@club) %>
